{
  "name": "ab/lx-parser",
  "description": "A modern yet time-tested monadic recursive descent parsing toolkit for any kind of text processing.",
  "keywords": [
    "parser",
    "parsing",
    "lexer",
    "compiler",
    "dsl",
    "doctrine",
    "hoa"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "A.B. Carroll",
      "homepage": "https://ab.hl9.net",
      "email": "ben@hl9.net",
      "role": "Current Maintainer"
    },
    {
      "name": "ferno",
      "homepage": "https://qntm.org/loco",
      "role": "Original Author"
    }
  ],
  "homepage": "http://qntm.org/loco",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.54",
    "phpunit/phpunit": "^9.4",
    "vimeo/psalm": "^4.1",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "infection/infection": "^0.20.1",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "scripts": {
    "check": "phpcs",
    "fix-cs": "phpcbf"
  },
  "autoload": {
    "psr-4": {
      "Ab\\LocoX\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ferno\\Tests\\Loco\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
