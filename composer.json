{
  "name": "ab/locoX-parser",
  "description": "The qntm.org loco lexer-parser (ferno/loco) modernized for PHP7.4+",
  "keywords": [
    "parser",
    "parsing",
    "lexer",
    "compiler",
    "dsl",
    "doctrine",
    "hoa"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "A.B. Carroll",
      "homepage": "https://ab.hl9.net",
      "email": "ben@hl9.net",
      "role": "Current Maintainer"
    },
    {
      "name": "ferno",
      "email": "actinium@gmail.com",
      "homepage": "https://qntm.org/loco",
      "role": "Original Author"
    }
  ],
  "homepage": "http://qntm.org/loco",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.3",
    "jakub-onderka/php-parallel-lint": "0.*"
  },
  "scripts": {
    "check": "phpcs",
    "fix-cs": "phpcbf"
  },
  "autoload": {
    "psr-4": {
      "Ferno\\Loco\\": "src/"
    },
    "files": [
      "src/serialize_array.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Ferno\\Test\\Loco\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
