{
  "name": "ab/locox-parser",
  "description": "The qntm.org loco lexer-parser (ferno/loco) modernized for PHP7.4+",
  "keywords": [
    "parser",
    "parsing",
    "lexer",
    "compiler",
    "dsl",
    "doctrine",
    "hoa"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "A.B. Carroll",
      "homepage": "https://ab.hl9.net",
      "email": "ben@hl9.net",
      "role": "Author, Maintainer"
    },
    {
      "name": "ferno",
      "email": "actinium@gmail.com",
      "homepage": "https://qntm.org/loco",
      "role": "Author"
    }
  ],
  "homepage": "http://qntm.org/loco",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "filp/whoops": "^2.7",
    "symfony/var-dumper": "^5.0",
    "ab/devel-quality": "dev-develop",
    "roave/psalm-html-output": "^0.0.2",
    "dq5studios/psalm-junit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ferno\\loco": "src/",
      "ferno\\loco\\test": "src/tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ab\\Tests\\LocoX\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
